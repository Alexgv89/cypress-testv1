{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "@cypress/xpath",
            "node"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node16",
        "module": "node16"
    },
    "include": [
        "**/*.ts",
        "**/*.js",
        "cypress.config.ts"
    ]
}