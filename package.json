{
  "name": "cypress-testv1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear:results": "rimraf --glob allure-results allure-report",
    "clear:history": "rimraf --glob allure-history",
    "cy:run": "npx allure run --config allurerc.json -- npx cypress run --spec 'cypress/e2e/test/**/*.cy.ts'",
    "report:generate": "npx allure generate --config allurerc.json allure-results -o allure-report ",
    "report:open": "npx allure open allure-report --port 8082",
    "test": "npm run clear:results && npm run cy:run  && npm run report:generate && npm run report:open"
  },
  "keywords": [],
  "author": "ing. Alexander E. Gonzalez",
  "license": "ISC",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "allure": "^3.1.0",
    "allure-cypress": "^3.4.5",
    "allure-mocha": "^3.4.5",
    "cypress": "^15.9.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-on-fix": "^1.1.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.1",
    "mochawesome-report-generator": "^6.3.2",
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "mocha": "^11.3.0"
  }
}